5.8
create database student_21;
use student_21;
create table student(
studentname varchar(15),
regno varchar(5),
dob date,
city varchar(15)
);
insert into student(studentname,regno,dob,city)
values
('layaa','123','2009-11-23','salwar'),
('shabana','456','2019-03-06','salem'),
('balaji','789','2012-05-05','chennai'),
('akash','101','2020-11-09','tpt'),
('shalini','112','2010-12-14','vkp');
select*from student
where city like'%sal%';

5.9
CREATE DATABASE COLLEGE_44;
USE COLLEGE_44;
CREATE TABLE STUDENT(
REGNO VARCHAR(15) PRIMARY KEY,
S1NAME VARCHAR(50) NOT NULL,
DOB DATE,
DEPTNO VARCHAR(10),
GENDER VARCHAR(10),
ADDRESS VARCHAR(100)
);
INSERT INTO STUDENT (RegNo, S1Name, DOB, DeptNo, Gender, Address)
VALUES 
('191711341', 'SUJAY', '2002-04-15', 'D01', 'Male', 'Salem'),
('191711342', 'POOJA', '2001-11-22', 'D02', 'Female', 'Salem'),
('191711343', 'GRISHA', '2002-06-10', 'D01', 'Male', 'Madurai'),
('191711344', 'ARTHI', '2001-09-03', 'D03', 'Female', 'coimbatore');
SELECT RegNo, S1Name, DOB, DeptNo, Gender, Address
FROM STUDENT
WHERE LENGTH (S1name)<>'6';

5.10
CREATE DATABASE COLLEGE_46;
USE COLLEGE_46;
CREATE TABLE STUDENT(
REGNO VARCHAR(15) PRIMARY KEY,
S1NAME VARCHAR(50) NOT NULL,
DOB DATE,
DEPTNO VARCHAR(10),
GENDER VARCHAR(10),
ADDRESS VARCHAR(100)
);
INSERT INTO STUDENT (RegNo, S1Name, DOB, DeptNo, Gender, Address)
VALUES 
('191711341', 'PRIYA', '2002-04-15', 'D01', 'Male', 'Salem'),
('191711342', 'SHAM', '2001-11-22', 'D02', 'Female', 'Salem'),
('191711343', 'AMRITHA', '2002-06-10', 'D01', 'Male', 'Madurai'),
('191711344', 'RAM', '2001-09-03', 'D03', 'Female', 'coimbatore');
SELECT RegNo, S1Name, DOB, DeptNo, Gender, Address
FROM STUDENT
WHERE S1Name like '%AM%';
